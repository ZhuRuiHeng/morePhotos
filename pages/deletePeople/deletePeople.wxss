/* pages/deletePeople/deletePeople.wxss */

.group {
  padding-bottom: 67px;
}

.checkbox {
  display: -webkit-flex;
  display: flex;
  width: 351px;
  margin-left: 8px;
  height: 52px;
  -webkit-align-items: center;
  align-items: center;
  position: relative;
}

.checkbox {
  border-bottom:1px solid rgba(136, 136, 136, 0.5);
}

.checkbox:nth-last-of-type(1) {
  border-bottom:none;
}

.select {
  left: 0;
  width: 40px;
  height: 48px;
  background: url('data:image/png;base64,') no-repeat center left;
  background-size: 22px 22px;
  background-color: #fff;
  z-index: 100;
}

.noselect {
  left: 0;
  width: 40px;
  height: 48px;
  background: url('data:image/png;base64,') no-repeat center left;
  background-size: 22px 22px;
  background-color: #fff;
  z-index: 100;
}

.gpic {
  width: 34px;
  height: 34px;
  border-radius: 17px;
  margin-right: 21px;
}

.name {
  font-size: 15px;
}

.btn {
  position: fixed;
  bottom: 0;
  z-index: 100;
  width: 100%;
  height: 67px;
  background: #fff;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.delete {
  background: #888;
  color: #fff;
  font-size: 17px;
  width: 336px;
}

.btn .delete.on {
  background: #000;
}
