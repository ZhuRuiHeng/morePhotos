  justify-content: center;
  align-items: flex-end;
  -webkit-animation: bgColor 0.5s ease;
  animation: bgColor 0.5s ease;
  background: rgba(0, 0, 0, 0.5);
}

@-webkit-keyframes bgColor {
  0% {
    background: rgba(0, 0, 0, 0);
  }

  100% {
    background: rgba(0, 0, 0, 0.5);
  }
}

@keyframes bgColor {
  0% {
